{
    "manifest_version": 3,
    "name": "Sentence Repeat Autofill Extension",
    "version": "1.0",
    "description": "Fills text boxes, checks checkboxes, and clicks buttons.",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  